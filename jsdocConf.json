{
    "tags": {
      "allowUnknownTags": true,
      "dictionaries": ["jsdoc"]
    },
    "source": {
      "include": ["app", "components", "context", "lib", "README.md"],
      "includePattern": ".jsx?$",
      "excludePattern": "(node_modules/|docs)"
    },
    "sourceType":"module",
    "plugins": ["plugins/markdown"],
    "templates": {
      "cleverLinks": false,
      "monospaceLinks": false
    },
    "opts": {
      "encoding": "utf8",
      "destination": "./docs/",
      "recurse": true
    }
   
  }
  